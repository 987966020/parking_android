<?xml version="1.0" encoding="utf-8"?>
  <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout_plh"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:focusableInTouchMode="false"
    android:focusable="false"
    android:background="@color/btn_white_normal"
    >
    <LinearLayout

        android:layout_width="fill_parent"
        android:layout_height="40dip"
        android:orientation="horizontal"
        >
        <TextView
            android:id="@+id/tv_content_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tv_current_car_type"
            android:textSize="16sp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            />
        <TextView
            android:id="@+id/tv_current_Type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/tv_current_car_change_type"
            android:textSize="14sp"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"
            />
    </LinearLayout>
</LinearLayout>
