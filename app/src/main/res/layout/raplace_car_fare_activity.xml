<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/liner_focus"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/btn_white_normal"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dip"
        >
        <TextView
            android:id="@+id/tv_replace_car_fare_text"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/green"
            android:text="@string/tv_replace_car_fare"
            android:textSize="20sp"
            android:textColor="@color/title_color"
            android:gravity="center"
            />
        <hbie.vip.parking.button.WaveView
            android:id="@+id/relativeLayout_replace_car_fare_back"
            android:layout_width="50dip"
            android:layout_height="match_parent"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/ic_fanhui_left"
                android:layout_centerInParent="true"
                />
        </hbie.vip.parking.button.WaveView>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="60dip"
        android:background="@color/title_color"
        >

        <RelativeLayout
            android:id="@+id/relativeLayout_rpcar_Num"
            android:layout_width="match_parent"
            android:layout_height="30dip"
            android:layout_centerVertical="true"
            android:layout_marginRight="70dip"
            android:layout_marginLeft="10dip"
            android:background="@color/btn_white_normal"
            android:focusable="true"
            android:focusableInTouchMode="true"
            >
            <RelativeLayout
                android:id="@+id/relativeLayout_rp_Num_center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                >
            <TextView
                android:id="@+id/et_rp_Num_center"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/et_search_replace_car_Num"
                android:textSize="16sp"
                android:background="@null"
                android:drawableLeft="@drawable/ic_chazhao"
                android:layout_centerVertical="true"
                android:visibility="visible"

                />
            </RelativeLayout>
            <!--<RelativeLayout-->
                <!--android:id="@+id/relativeLayout_rp_Num_left"-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="wrap_content"-->
                <!--android:layout_centerVertical="true"-->
                <!--android:visibility="gone"-->
                <!--android:focusable="true"-->
                <!--android:focusableInTouchMode="true"-->
                <!-->-->
                <EditText
                    android:id="@+id/et_write_rpcar_Num_left"
                    android:layout_width="match_parent"
                    android:layout_height="30dip"
                    android:textSize="16sp"
                    android:background="@null"
                    android:layout_centerVertical="true"
                    android:paddingLeft="30dip"
                    />
            <requestFocus />
            <ImageView
                android:id="@+id/image_search_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/ic_chazhao"
                android:layout_centerVertical="true"
                android:visibility="gone"
                />
            <!--</RelativeLayout>-->
        </RelativeLayout>
        <Button
            android:id="@+id/rp_search_car_fare_nymber"
            android:layout_width="60dip"
            android:layout_height="30dip"
            android:background="@drawable/btn_search"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:text="查询"
            android:textColor="@color/title_color"
            android:textSize="16sp"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dip"
        android:layout_marginTop="10dip"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    <ListView
        android:id="@+id/lv_rpcar_pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
    <ImageView
        android:id="@+id/lv_comprehensive_nullimage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/kongkatong"
        android:visibility="gone"
        />
   </RelativeLayout>
    <!--<include layout="@layout/nomalcar_order_list_unpay"/>-->
</LinearLayout>
